<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_browse_fragment"
    android:layout_width="match_parent"
    android:background="#ECEDFF"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:deviceIds="tv"
    android:padding="10dp"
    tools:ignore="MergeRootFrame"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:weightSum="10"
        android:layout_marginBottom="40dp"
        android:baselineAligned="false">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:gravity="center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/stock_title"
                android:gravity="start"
                android:textSize="25sp"
                android:fontFamily="@font/nanumgothicextrabold"
                android:layout_marginStart="10dp"
                android:textColor="#000000">

            </TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="7"
            android:baselineAligned="false"
            android:weightSum="6">
            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3">
            </View>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:baselineAligned="false"
                android:layout_weight="2">

                <EditText
                    android:id="@+id/edit_stock"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:drawableStart="@drawable/ic_baseline"
                    android:enabled="false"
                    android:textColor="#000000"
                    android:hint="@string/stock_hint"
                    android:textColorHint="#000000"
                    android:inputType="text">

                </EditText>
            </LinearLayout>
            <Button
                android:id="@+id/button_crypto"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:enabled="false" />

            <Button
                android:id="@+id/getVoiceTextButton"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/stock_button"
                android:textAllCaps="false"
                android:textSize="12sp"
                android:layout_weight="1"
                />

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:baselineAligned="false"
        android:layout_marginTop="10dp"
        android:weightSum="11">



            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="6"
                android:weightSum="10"
                android:layout_marginStart="0dp"
                android:padding="20dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0"
                    android:orientation="horizontal"
                    android:weightSum="8">

                    <Button
                        android:id="@+id/search_stock"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:enabled="false" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:weightSum="5"
                    android:background="@drawable/index_status_left"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/stock_name"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:padding="3dp"
                        android:layout_marginStart="10dp"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        android:text="@string/blank"/>

                    <View
                        android:id="@+id/stock_bar"
                        android:layout_width="3dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:layout_height="match_parent"
                        android:background="#000000" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:orientation="vertical"
                        android:weightSum="8">

                        <TextView
                            android:id="@+id/stock_price"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textColor="#000000"
                            android:layout_weight="5" />

                        <TextView
                            android:id="@+id/stock_percent"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:textColor="#000000"
                            android:layout_weight="3" />


                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:padding="3dp"
                        android:layout_weight="1"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="5"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/index_status_left">
                    <com.github.mikephil.charting.charts.CandleStickChart
                        android:id="@+id/priceChart"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="5dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="5dp"
                        android:layout_marginEnd="15dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="4"
                    android:layout_marginTop="10dp"
                    android:weightSum="3"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:weightSum="3"
                        android:layout_margin="5dp"
                        android:orientation="vertical">

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:layout_marginTop="5dp"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:layout_marginStart="3dp"
                                    android:gravity="center_vertical"
                                    android:text="시가"/>
                                <TextView
                                    android:id="@+id/opening_price"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_marginStart="3dp"
                                    android:gravity="center_vertical"
                                    android:layout_weight="3"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    android:text="고가"/>
                                <TextView
                                    android:id="@+id/high_price"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_weight="3"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:layout_marginTop="5dp"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    android:text="저가"/>
                                <TextView
                                    android:id="@+id/low_price"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_weight="3"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_margin="5dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:weightSum="3"
                        android:orientation="vertical">

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    android:text="52주 신고가"/>
                                <TextView
                                    android:id="@+id/highest_52_price"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_weight="3"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    android:text="52주 신저가"/>
                                <TextView
                                    android:id="@+id/lowest_52_price"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_weight="3"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    android:text="배당률"/>
                                <TextView
                                    android:id="@+id/dividend"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_weight="3"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:weightSum="3"
                        android:layout_margin="5dp"
                        android:orientation="vertical">

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    android:text="PER"/>
                                <TextView
                                    android:id="@+id/per"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_weight="3"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    android:text="EPS"/>
                                <TextView
                                    android:id="@+id/eps"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_weight="3"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            app:cardCornerRadius="5dp"
                            app:cardElevation="0dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@color/white"
                                android:weightSum="5"
                                android:orientation="vertical"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:layout_weight="2"
                                    android:textStyle="normal"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    android:text="PBR"/>
                                <TextView
                                    android:id="@+id/pbr"
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:textColor="#000000"
                                    android:textStyle="bold"
                                    android:layout_weight="3"
                                    android:gravity="center_vertical"
                                    android:layout_marginStart="3dp"
                                    />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:weightSum="2"
                android:layout_margin="10dp"
                android:layout_weight="1">
            </LinearLayout>


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:weightSum="7"
                android:layout_marginStart="0dp"
                android:padding="20dp"
                android:layout_weight="4">



                <androidx.cardview.widget.CardView
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:weightSum="5"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:layout_weight="2"
                            android:textStyle="normal"
                            android:gravity="center"
                            android:layout_marginStart="3dp"
                            android:text="KOSPI"/>
                        <TextView
                            android:id="@+id/kospi_index"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:layout_marginStart="3dp"
                            android:text="test"/>
                        <TextView
                            android:id="@+id/kospi_rate"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:layout_marginStart="3dp"
                            android:text="test"/>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="0dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:weightSum="5"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:layout_weight="2"
                            android:textStyle="normal"
                            android:gravity="center"
                            android:layout_marginStart="3dp"
                            android:text="KOSDAQ"/>
                        <TextView
                            android:id="@+id/kosdaq_index"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:layout_marginStart="3dp"
                            android:text="test"/>
                        <TextView
                            android:id="@+id/kosdaq_rate"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:layout_marginStart="3dp"
                            android:text="test"/>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:weightSum="5"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:layout_weight="2"
                            android:textStyle="normal"
                            android:gravity="center"
                            android:layout_marginStart="3dp"
                            android:text="SnP500"/>
                        <TextView
                            android:id="@+id/snp500_price"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:layout_marginStart="3dp"
                            android:text="test"/>
                        <TextView
                            android:id="@+id/snp500_rate"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:layout_marginStart="3dp"
                            android:text="test"/>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:weightSum="5"
                        android:orientation="horizontal"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:layout_weight="2"
                            android:textStyle="normal"
                            android:gravity="center"
                            android:layout_marginStart="3dp"
                            android:text="Nasdaq"/>
                        <TextView
                            android:id="@+id/Nasdaq_price"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:layout_marginStart="3dp"
                            android:text="test"/>
                        <TextView
